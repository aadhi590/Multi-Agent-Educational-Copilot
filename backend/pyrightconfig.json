{
    "venvPath": ".",
    "venv": "venv",
    "pythonVersion": "3.13",
    "pythonPlatform": "Windows",
    "include": [
        "."
    ],
    "extraPaths": [
        "."
    ],
    "exclude": [
        "venv",
        "__pycache__",
        "*.pyc"
    ],
    "typeCheckingMode": "basic",
    "reportMissingImports": "error",
    "reportMissingModuleSource": "none"
}